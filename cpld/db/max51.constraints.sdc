#**************************************************************
# Create Clock
#**************************************************************

create_clock -name clk_sys -period 25.000 -waveform { 0.000 12.500 } $_col0


#**************************************************************
# Create Generated Clock
#**************************************************************

create_generated_clock -name spi_clk \
                       -source $_col0 \
                       -edges {1 1 1} \
                       -edge_shift {0.000 12.500 25.000} \
                       -master_clock clk_sys \
                       -add \
                       $_col1


#**************************************************************
# Set Propagated Clock
#**************************************************************

set_propagated_clock [all_clocks]


